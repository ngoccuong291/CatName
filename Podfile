source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '10.0'
inhibit_all_warnings!

target :CatNames do

	use_frameworks!

	# Third party libraries
	pod 'Alamofire', '~> 4.7'
	pod 'AlamofireObjectMapper', '~> 5.0'
	pod 'SVProgressHUD', '~> 2.2'

	target 'CatNamesTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick', '~> 1.2.0'
    pod 'Nimble', '~> 7.0.2'

  end

end
